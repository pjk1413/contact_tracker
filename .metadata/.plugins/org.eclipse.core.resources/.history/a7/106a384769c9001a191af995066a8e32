package com.kramer.contactTracker.models;

import javax.persistence.*;

@Entity
public class Address {

	@Id
	@GeneratedValue
	@Column(name = "address_id")
	private Long id;
	
	@Column(name = "address")
	private String address;
	
	@Column(name = "city")
	private String city;
	
	@Column(name = "state")
	private String state;
	
	@Column(name = "zipcode")
	private String zipcode;
	
	@Column(name = "country")
	private String country;
}
